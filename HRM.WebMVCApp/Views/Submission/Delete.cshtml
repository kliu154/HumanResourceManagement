@model SubmissionResponseModel

<form asp-action="Delete" asp-controller="Submission" method="post">
    <div class="container">
        <div class="row">
            <label asp-for="CandidateId"></label>
            <span>@Model.CandidateId</span>
        </div>
        <div class="row">
            <label asp-for="JobRequirementId"></label>
            <span>@Model.JobRequirementId</span>
        </div>
        <input type="submit" value="Delete" class="btn btn-danger" />
    </div>
</form>
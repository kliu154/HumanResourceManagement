@model IEnumerable<InterviewStatusResponseModel>;
<div class="page-header">
    <h1>Interview Status</h1>
</div>
<a asp-action="Create" asp-controller="InterviewStatus">Add New Interview Status</a>

<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
                Is Acitve
            </th>
            <th colspan="2">
                Options
            </th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var m in Model)
            {
                                        <tr>
                                            <td>@m.Title</td>
                                            <td>@m.IsActive</td>
                                            <td>
                                                <a asp-action="Edit" asp-controller="InterviewStatus" asp-route-id="@m.Id">Edit</a>
                                            </td>
                                            <td>
                                                <a asp-action="Delete" asp-controller="InterviewStatus" asp-route-id="@m.Id">Delete</a>
                                            </td>
                                        </tr>
            }
        }
    </tbody>
</table>
@model JobCategoryResponseModel

<form asp-action="Delete" asp-controller="JobCategory" method="post">
    <div class="container">
        <div class="row">
            <label asp-for="Name"></label>
            <span>@Model.Name</span>
        </div>
        <div class="row">
            <label asp-for="IsActive"></label>
            <span>@Model.IsActive</span>
        </div>
        <input type="submit" value="Delete" class="btn btn-danger" />
    </div>
</form>
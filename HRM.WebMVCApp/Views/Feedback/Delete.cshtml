@model FeedbackResponseModel

<form asp-action="Delete" asp-controller="Feedback" method="post">
    <div class="container">
        <div class="row">
            <label asp-for="InterviewId"></label>
            <span>@Model.InterviewId</span>
        </div>
        <div class="row">
            <label asp-for="Description"></label>
            <span>@Model.Description</span>
        </div>
        <input type="submit" value="Delete" class="btn btn-danger" />
    </div>
</form>
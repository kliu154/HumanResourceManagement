@model InterviewResponseModel

<form asp-action="Delete" asp-controller="Interview" method="post">
    <div class="container">
        <div class="row">
            <label asp-for="InterviewerId"></label>
            <span>@Model.InterviewerId</span>
        </div>
        <div class="row">
            <label asp-for="SubmissionId"></label>
            <span>@Model.SubmissionId</span>
        </div>
        <input type="submit" value="Delete" class="btn btn-danger" />
    </div>
</form>
@model IEnumerable<InterviewResponseModel>;
<div class="page-header">
    <h1>List of Interviews</h1>
</div>
<a asp-action="Create" asp-controller="Interview">Add New Interview</a>

<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            <th>
                Submission
            </th>
            <th>
                Interview Date
            </th>
            <th>
                Interview Round
            </th>
            <th>
                Interview Type
            </th>
            <th>
                Interview Status
            </th>
            <th>
                Interviewer
            </th>
            <th colspan="2">
                Options
            </th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var m in Model)
            {
                        <tr>
                            <td>@m.SubmissionId</td>
                            <td>@m.InterviewDate</td>
                            <td>@m.InterviewRound</td>
                            <td>@m.InterviewTypeId</td>
                            <td>@m.InterviewStatusId</td>
                            <td>@m.InterviewerId</td>
                            <td>
                                <a asp-action="Edit" asp-controller="Interview" asp-route-id="@m.Id">Edit</a>
                            </td>
                                <td><a asp-action="Delete" asp-controller="Interview" asp-route-id="@m.Id">Delete</a></td>
                        </tr>
            }
        }
    </tbody>
</table>